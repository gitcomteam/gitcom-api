paths:
    migrations: '%%PHINX_CONFIG_DIR%%/db/migrations'
    seeds: '%%PHINX_CONFIG_DIR%%/db/seeds'

environments:
    default_migration_table: phinxlog
    default_database: testing
    testing:
        adapter: pgsql
        host: localhost
        name: openhub_api_test
        user: postgres
        pass: ''
        port: 5432
        charset: utf8

version_order: creation